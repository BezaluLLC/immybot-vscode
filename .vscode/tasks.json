// See https://go.microsoft.com/fwlink/?LinkId=733558
// for the documentation about the tasks.json format
{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "npm",
			"script": "compile-web",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			
		},
		{
			"label": "browser-debug",
			"type": "npm",
			"script": "debug",
			"group": "build",
			"isBackground": false,
		},
		{
			"label": "create-test-workspace",
			"type": "shell",
			"command": "mkdir",
			"args": ["-p", "${workspaceFolder}/.vscode/test-workspace"],
			"group": "build",
			"presentation": {
				"echo": false,
				"reveal": "never",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": false,
				"clear": false
			},
			"dependsOn": "npm: compile-web",
		}
	]
}